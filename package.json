{
  "name": "icxr-suite",
  "description": "A Discord bot and management system for ICXR",
  "version": "0.1.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "gauth": "ts-node src/server/google-oauth.ts"
  },
  "dependencies": {
    "discord.js": "^14.12.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^125.0.0",
    "payload": "^1.12.0",
    "payload-discord": "link:..\\payload-discord"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "private": true
}
